{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "cf_token",
    "uptimerobot_token",
    "uptimerobot_contactid",
    "admin_ip"
  ],
  "properties": {
    "cf_token": {
      "type": "string",
      "minLength": 1
    },
    "uptimerobot_token": {
      "type": "string",
      "minLength": 1
    },
    "uptimerobot_contactid": {
      "type": "string",
      "additionalProperties": {
        "type": "object",
        "required": ["email", "call"],
        "properties": {
          "email": {
            "type": "string",
            "minLength": 1
          },
          "call": {
            "type": "string",
            "minLength": 1
          }
        },
        "additionalProperties": false
      }
    },
    "admin_ip": {
      "type": "string",
      "minLength": 1
    }
  },
  "additionalProperties": false
}